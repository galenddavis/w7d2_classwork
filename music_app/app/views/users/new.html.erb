<h1>Make a new music</h1>

<form action='<%= users_url %>' method='post'>
    <%= auth_token %>
        <label for='user_email'>Email</label>
        <input id='user_email' type='text' name='user[email]'>

        <label for='new_password'>Password</label>
        <input id='new_password' type='password' name='user[password]'>

        <input type='submit' value='New Kai'>
        <%= render 'shared/errors' %>


        
</form>
